            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCBHCOLBY9rf4XqGblwRjA_zOnc4_TlRl4" />

        <activity
            android:name=".view.BaseActivity"
            android:exported="true"
            android:label="@string/app_name"
